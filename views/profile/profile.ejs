
<div class="content clearfix">
  <header class="profile__bio">
    <img src="<%= user.avatar_url %>" alt="ESFiddle Avatar" class="profile__img">

    <div class="profile__details">
      <h1><%= user.name %></h1>
      <h3><%= user.location %></h3>
      <h3><a href='<%= user.html_url %>'>github.com/<%= user.login %></a></h3>
    </div>
  </header>
  <div class="main">
    <% include recentFiddles %>
    <% include starredFiddles %>
  </div>
</div><!-- @end #content -->
